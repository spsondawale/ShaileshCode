{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "mangliadf"
		},
		"ls_copy_file_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_copy_file'"
		},
		"ls_databricks_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'ls_databricks'"
		},
		"ls_file_copy_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_file_copy'"
		},
		"ls_file_copy_adls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_file_copy_adls'"
		},
		"ls_copy_file_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mangliretail.dfs.core.windows.net/"
		},
		"ls_databricks_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0904-121411-b30w5hp3"
		},
		"ls_file_copy_adls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mangliretail.dfs.core.windows.net/"
		},
		"ls_new_link_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1005-030659-ivpdva9o"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "mangliadf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_copy_file_accountKey": {
						"value": "[parameters('ls_copy_file_accountKey')]"
					},
					"ls_databricks_accessToken": {
						"value": "[parameters('ls_databricks_accessToken')]"
					},
					"ls_file_copy_connectionString": {
						"value": "[parameters('ls_file_copy_connectionString')]"
					},
					"ls_file_copy_adls_accountKey": {
						"value": "[parameters('ls_file_copy_adls_accountKey')]"
					},
					"ls_copy_file_properties_typeProperties_url": {
						"value": "[parameters('ls_copy_file_properties_typeProperties_url')]"
					},
					"ls_databricks_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_databricks_properties_typeProperties_existingClusterId')]"
					},
					"ls_file_copy_adls_properties_typeProperties_url": {
						"value": "[parameters('ls_file_copy_adls_properties_typeProperties_url')]"
					},
					"ls_new_link_properties_typeProperties_existingClusterId": {
						"value": "[parameters('ls_new_link_properties_typeProperties_existingClusterId')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}