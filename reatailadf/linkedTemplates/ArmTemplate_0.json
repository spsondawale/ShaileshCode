{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "reatailadf"
		},
		"ls_copy_file_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_copy_file'"
		},
		"ls_copy_file_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://mangliretail.dfs.core.windows.net/"
